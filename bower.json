{
  "name": "newProjectBundle",
  "version": "0.0.0",
  "dependencies": {
    "jquery": "*",
    "fancybox": "latest",
    "bootstrap4-grid-only": "latest",
    "animate.css": "latest",
    "jquery.maskedinput": "latest",
    "jquery-selectric": "latest",
    "owl.carousel": "latest",
    "hover": "latest",
    "flexslider": "latest",
    "components-font-awesome": "latest",
    "nouislider": "latest",
    "air-datepicker": "latest",
    "webcomponentsjs": "latest"
  },
  "overrides": {
    "components-font-awesome": {
      "src/css/awesome": [
        "./css/fontawesome-all.css"
      ],
      "src/css/webfonts": [
        "./webfonts/*"
      ]
    },
    "bootstrap4-grid-only": {
      "src/css/grid": [
        "./dist/css/bootstrap-grid.css"
      ]
    },
    "air-datepicker": {
      "src/js/vendors/air-datepicker": [
        "./dist/**"
      ]
    },
    "jquery": {
      "src/js/vendors/jquery": [
        "./dist/jquery.min.js"
      ]
    },
    "fancybox": {
      "src/js/vendors/fancybox": [
        "./dist/jquery.fancybox.css",
        "./dist/jquery.fancybox.js"
      ]
    },
    "animate.css": {
      "src/css/animate": [
        "./animate.css",
        "./animate.min.css"
      ]
    },
    "jquery.maskedinput": {
      "src/js/vendors/mask": [
        "./dist/jquery.maskedinput.min.js"
      ]
    },
    "jquery-selectric": {
      "src/js/vendors/selectric": [
        "./public/jquery.selectric.min.js",
        "./public/selectric.css"
      ]
    },
    "owl.carousel": {
      "src/js/vendors/owl": [
        "./dist/owl.carousel.js",
        "./dist/assets/*"
      ]
    },
    "hover": {
      "src/css/hover": [
        "./css/hover.css",
        "./css/hover-min.css"
      ]
    },
    "flexslider": {
      "src/js/vendors/flex": [
        "./flexslider.css",
        "./jquery.flexslider.js",
        "./jquery.flexslider-min.js",
        "./images/*",
        "./fonts/*"
      ]
    },
    "nouislider": {
      "src/js/vendors/nouislider": [
        "./distribute/nouislider.js",
        "./distribute/nouislider.min.css",
        "./distribute/nouislider.min.js"
      ]
    }
  }
}