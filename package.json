{
  "name": "webpack4-dev-kit",
  "version": "1.0.0",
  "description": "Site boilerplate on webpack4",
  "main": "index.ts",
  "scripts": {
    "build": "webpack --config wp-config/webpack.prod.js",
    "start": "webpack-dev-server --open --config wp-config/webpack.dev.js",
    "ts-check": "tsc --noEmit",
    "prettier": "prettier --write '**/*.{ts,tsx,js}'",
    "fix-code-style": "npm run prettier && npm run lint:fix",
    "lint:fix": "eslint --fix . --ext .tsx,.ts,.js . --quiet",
    "lint": "eslint -c .eslintrc.js  --quiet '!(node_modules|dist)/**/*.{ts,tsx,js}'",
    "check": "npx npm-check-updates -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abaddonGIT/WebPackAndGulp.git"
  },
  "keywords": [
    "webpack",
    "webpack-4",
    "webpack 4",
    "webpack 4 (legato)",
    "bundle",
    "es6",
    "devkit",
    "sass"
  ],
  "author": "Abaddon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abaddonGIT/WebPackAndGulp/issues"
  },
  "homepage": "https://github.com/abaddonGIT/WebPackAndGulp#readme",
