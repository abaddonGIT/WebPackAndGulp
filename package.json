{
  "name": "abba-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Site boilerplate on webpack5",
  "main": "index.js",
  "author": "Abaddon",
  "bugs": {
    "url": "https://github.com/abaddonGIT/WebPackAndGulp/issues"
  },
  "homepage": "https://github.com/abaddonGIT/WebPackAndGulp#readme",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js --hot",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint abba-webpack-boilerplate src config || true",
    "ts-check": "tsc --noEmit",
    "prettier": "prettier --write '**/*.{ts,tsx,js}'",
    "fix-code-style": "npm run prettier && npm run lint:fix",
    "lint:fix": "eslint --fix . --ext .tsx,.ts,.js . --quiet",
    "check": "npx npm-check-updates -u"
  },
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack boilerplate"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@fancyapps/fancybox": "^3.5.7",
    "@types/jquery": "^3.5.1",
    "@types/svg4everybody": "^2.1.1",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "air-datepicker": "^2.2.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-webpack-plugin": "^2.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-beta.1",
    "jquery": "^3.5.1",
    "jquery-migrate": "^3.3.1",
    "jquery-selectric": "^0.2.0",
    "jquery.maskedinput": "^1.4.1",
    "mini-css-extract-plugin": "^1.0.0",
    "node-sass": "^4.14.1",
    "nouislider": "^14.6.1",
    "owl.carousel": "^2.3.4",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "prettier-webpack-plugin": "^1.2.0",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.13",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:abaddonGIT/abba-webpack-boilerplate.git"
  },
  "dependencies": {
    "@kooneko/livereload-webpack-plugin": "^1.2.0",
    "glob": "^7.1.6",
    "offline-plugin": "^5.0.7",
    "raw-loader": "^4.0.2",
    "svg-sprite-loader": "^5.2.1",
    "svg4everybody": "^2.1.9",
    "svgo-loader": "^2.2.1",
    "normalize.css": "^8.0.1"
  }
}
